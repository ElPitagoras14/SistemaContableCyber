                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="txtValorTotal" alignment="CENTER_RIGHT" editable="false" maxWidth="100.0" prefWidth="100.0" promptText="0.00">
