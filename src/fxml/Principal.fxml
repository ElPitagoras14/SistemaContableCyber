            </Button>
            <Button fx:id="btnServiciosBancarios" mnemonicParsing="false" onMouseClicked="#serviciosBancarios" prefWidth="200.0" text="Servicios Bancarios" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Label fx:id="txtTransaccion" layoutX="242.0" layoutY="43.0" text="Transacción Actual">
