                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btnServiciosNativos" mnemonicParsing="false" onMouseClicked="#serviciosNativos" prefWidth="200.0" text="Servicios Nativos" textAlignment="CENTER" wrapText="true">
