      <Circle fill="#76b2cf" layoutX="182.0" layoutY="511.0" radius="24.0" stroke="#85b1d8" strokeType="INSIDE" />
      <Circle fill="#76b2cf" layoutX="239.0" layoutY="559.0" radius="24.0" stroke="#85b1d8" strokeType="INSIDE" />
      <Label layoutX="43.0" layoutY="30.0" text="Servicios">
